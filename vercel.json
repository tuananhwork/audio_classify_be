{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/predict",
      "dest": "app/main.py",
      "methods": ["POST"]
    },
    {
      "src": "/health",
      "dest": "app/main.py",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ]
}
